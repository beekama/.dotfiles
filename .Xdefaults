!URxvt*background: #141c21
URxvt*foreground: white
URxvt*font: xft:bitstream vera sans mono:size=7:antialias=true
URxvt.transparent: true
! URxvt.shading: 0 to 99 darkens, 101 to 200 lightens
URxvt.shading: 10
!URxvt.font: xft:Anonymous Pro for Powerline:pixelsize=36
!URxvt.font: xft:Monofur for Powerline:pixelsize=36
!URxvt.font: xft:Bitstream Vera Sans Mono for Powerline:pixelsize=36
!URxvt.font: xft:DejaVu Sans Mono for Powerline:pixelsize=24
!URxvt.font: xft:Droid Sans Mono for Powerline:pixelsize=36
!URxvt.font: xft:Inconsolata for Powerline:pixelsize=36
!URxvt.font: xft:Terminus (TTF) for Powerline:pixelsize=36
!URxvt.font: xft:Inconsolata\-dz for Powerline:pixelsize=36
!URxvt.font: xft:Inconsolata\-g for Powerline:pixelsize=36
!URxvt.font: xft:Liberation Mono for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG L DZ for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG L for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG M DZ for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG M for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG S DZ for Powerline:pixelsize=36
!URxvt.font: xft:Meslo LG S for Powerline:pixelsize=36
!URxvt.font: xft:Source Code Pro for Powerline:pixelsize=36
!URxvt.font: xft:Terminess Powerline:pixelsize=32:style=bold
!URxvt.font: xft:Ubuntu Mono derivative Powerline:pixelsize=36
!URxvt.font: xft:Literation Mono Powerline:pixelsize=36
URxvt.iso14755: false
URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.perl-ext-common: default,selection-to-clipboard,font-size
URxvt.print-pipe: true
URxvt.urgentOnBell: true

!font-size extension
URxvt.keysym.C-Up:  font-size:increase
URxvt.keysym.C-Down:    font-size:decrease
URxvt.keysym.C-S-Up:    font-size:incglobal
URxvt.keysym.C-S-Down:  font-size:decglobal
URxvt.keysym.C-equal: font-size:reset
URxvt.keysym.C-slash: font-size:show
